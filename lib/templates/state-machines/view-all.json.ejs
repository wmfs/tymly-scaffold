{
<%- include('frags/title') %>
  "version": "1.0",
<%- include('frags/categories') %>
  "StartAt": "Finding",
  "States": {
    "Finding": {
      "Type": "Task",
      "InputPath": "$",
      "Resource": "module:finding",
      "ResourceConfig": {
        "modelId": "<%= model %>"
<% if (field != null) { %>
        ,
        "filter": {
          "where": {
            "<%= field %>": {
              "equals": "$.<%= field %>"
            }
          }
        }
<% } %>
      },
      "ResultPath": "$.formData",
      "Next": "AwaitingHumanInput"
    },
    "AwaitingHumanInput": {
      "Type": "Task",
      "Resource": "module:awaitingHumanInput",
      "ResourceConfig": {
        "uiType": "form",
        "uiName": "<%= uiName %>",
        "dataPath": "$.formData"
      },
      "End": true
    }
  },
<%- include('frags/restrictions') %>
}
