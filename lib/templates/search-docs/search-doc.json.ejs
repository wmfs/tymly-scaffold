{
  "modelId": "<%= model %>",
  "attributeMapping": {
    "id": "'<%= model %>#'::text || <%- primaryKeys %>",
    "docId": "<%- primaryKeys %>",
    "domain": "'search'::text",
    "docType": "'<%= namespace %>_<%= model %>'::text",
    "title": "<%= title %>::text",
    "description": "<%- description %>",
    "additionalTerms": "NULL::text[]",
    "category": "'<%= category %>'::text",
    "eventTimestamp": "NULL::timestamp with time zone",
    "point": "NULL::text",
    "activeEvent": "false::boolean",
    "author": "'<%= model %>'::text",
    "teams": "NULL::text[]",
    "roles": "'{<%- roles %>}'::text[]",
    "tags": "NULL::text[]",
    "language": "'ENG'::text",
    "rootAncestorId": "NULL::text",
    "sortString": "<%= sort %>::text",
    "launches": "NULL::text",
    "created": "_created::timestamp with time zone",
    "modified": "_modified::timestamp with time zone"
  }
}
